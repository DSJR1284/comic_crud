<h1><%= @comics.title%></h1>
<br>
<img src="<%= @comics.image_url %>" alt="<%= @comics.title %>" height="150" width="150">
<br>
<h2><%= @comics.blurb %></h2>
<h4><%= @comics.user.username%></h4>
<br>
<form method="get" action="/comics/<%= @comics.id %>">
    <input type="submit" value="Rewrite Story">
</form>
<br>
<form method="post" action="/comics/<%= @comics.id %>/delete" >
 <input type="hidden" id="hidden" name="_method" value="DELETE">
 <input type="submit" value="Remove Story">
</form>
 <br>
 <br>

 
 
